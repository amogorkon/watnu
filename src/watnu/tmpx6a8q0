
import rope.base.project
from rope.base import libutils

myproject = rope.base.project.Project(
    'F:/Dropbox (Privat)/code/watnu/master/src/watnu')
#myproject.validate('F:/Dropbox (Privat)/code/watnu/master/src/watnu/main.py')
myproject.validate()
ropy = libutils.path_to_resource(myproject, 'F:/Dropbox (Privat)/code/watnu/master/src/watnu/main.py')

from rope.refactor.rename import Rename
changes = Rename(myproject, ropy, 29168).get_changes('Editor', docs=True)
myproject.do(changes)

myproject.close()
